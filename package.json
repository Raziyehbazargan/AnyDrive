{
  "name": "any-drive",
  "version": "1.0.0",
  "description": "a project to upload files, images, etc using AWS S# and Dropbox api",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "istanbul cover --dir ./coverage _mocha --bail --recursive './test/**.js' --opts",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "babel": "babel -w src/ -d build/",
    "heroku-postbuild": "babel src/ -d build/",
    "test-debug": "DEBUG='anydrive*' istanbul cover --dir ./coverage _mocha --bail --recursive './test/**/*-test.js' --opts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnyDrive/AnyDrive.git"
  },
  "keywords": [
    "AWS",
    "S3",
    "API",
    "Nodejs",
    "MongoDB"
  ],
  "author": "Raziyeh Bazargan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnyDrive/AnyDrive/issues"
  },
  "homepage": "https://github.com/AnyDrive/AnyDrive#readme",
  "dependencies": {
    "aws-sdk": "^2.22.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "crypto": "0.0.3",
    "debug": "^2.6.1",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "http-errors": "^1.6.1",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.8.4",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "node-uuid": "^1.4.7",
    "path": "^0.12.7",
    "superagent": "^3.5.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.16.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  }
}
