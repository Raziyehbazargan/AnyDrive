{
  "name": "any-drive",
  "version": "1.0.0",
  "description": "a project to upload files, images, etc using AWS S# and Dropbox api",
  "main": "server.js",
  "scripts": {
    "start": "concurrently 'npm run server-start' 'npm run app-start'",
    "start-watch": "./node_modules/nodemon/bin/nodemon.js npm server-start",
    "server-start": "node server.js",
    "start-lite": "npm run lite",
    "lite": "lite-server",
    "app-start": "./node_modules/.bin/webpack-dev-server",
    "test": "istanbul cover --dir ./coverage _mocha --bail --recursive './test/**.js' --opts",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "babel": "babel -w src/ -d build/",
    "heroku-postbuild": "babel src/ -d build/",
    "test-debug": "DEBUG='anydrive*' istanbul cover --dir ./coverage _mocha --bail --recursive './test/**/*-test.js' --opts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnyDrive/AnyDrive.git"
  },
  "keywords": [
    "AWS",
    "S3",
    "API",
    "Nodejs",
    "MongoDB"
  ],
  "author": "Raziyeh Bazargan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnyDrive/AnyDrive/issues"
  },
  "homepage": "https://github.com/AnyDrive/AnyDrive#readme",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "aws-sdk": "^2.22.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.16.1",
    "clean-webpack-plugin": "^0.1.15",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "cowsay-browser": "^1.1.8",
    "crypto": "0.0.3",
    "css-loader": "^0.26.2",
    "debug": "^2.6.1",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "dropbox": "^2.5.2",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-errors": "^1.6.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.8.4",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "node-sass": "^4.5.0",
    "node-uuid": "^1.4.7",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "superagent": "^3.5.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^3.0.0",
    "coveralls": "^2.11.16",
    "debug": "^2.6.8",
    "eslint": "^3.16.1",
    "istanbul": "^0.4.5",
    "lite-server": "^2.2.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-bootstrap": "^0.31.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
